{
    "manifest_version": 3,
    "name": "Auto Refresh 1 Min",
    "description": "This extension will automatically refresh the page every 1 minute.",
    "version": "1.2",
    "permissions": [
        "activeTab",
        "alarms"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "icon48.png"
    },
    "action": {
        "default_icon": "icon128.png",
        "default_title": "Auto Refresh 1 Min"
    }
}

